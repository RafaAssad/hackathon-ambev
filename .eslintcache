Value={!city ? '' : city.neighborhood}\n          onChange={handlePersona}\n        />\n\n        <label htmlFor=\"number\">Numero:</label>\n        <input type=\"number\" name=\"number\" onChange={handlePersona} required />\n\n        <label htmlFor=\"complement\">Complemento:</label>\n        <input name=\"complement\" onChange={handlePersona} />\n\n        <label htmlFor=\"city\">Cidade:</label>\n        <input\n          name=\"city\"\n          defaultValue={!city ? '' : city.city}\n          onChange={handlePersona}\n          required\n        />\n\n        <label htmlFor=\"state\">Estado: </label>\n        <input\n          name=\"state\"\n          defaultValue={!city ? '' : city.state}\n          onChange={handlePersona}\n          required\n        />\n      </form>\n      <form className=\"flex-me\">\n        <label>Forma de pagamento:</label>\n        <select onClick={(e) => setMetod(e.target.value)}>\n          <option value=\"\" disabled>\n            Selecione um opcao\n          </option>\n          <option value=\"credit\">Cartão de crédito</option>\n          <option value=\"boleto\">Boleto</option>\n          <option value=\"transfer\">Transferência</option>\n          <option value=\"pix\">Pix</option>\n        </select>\n        <FormaPagamento forma={metod} />\n      </form>\n    </div>\n  );\n}\n\nexport default Pagamento;\n","/home/paulo/Projetos para vagas/hackathon/src/components/Header.jsx",[],"/home/paulo/Projetos para vagas/hackathon/src/components/db.jsx",[],"/home/paulo/Projetos para vagas/hackathon/src/components/formasPagamento.jsx",["107"],"import React from 'react';\nimport '../CSS/pagamento.css';\nfunction FormaPagamento(props) {\n  console.log(props);\n  switch (props.forma) {\n    case 'credit':\n      return (\n        <div className=\"flex-me\">\n          <label htmlFor=\"cred-name\">Nome impresso no cart'ao: </label>\n          <input type=\"text\" name=\"cred-name\" />\n          <div>\n            <label htmlFor=\"cred-number\">Numero do cart'ao: </label>\n            {/* <input type=\"text\" name=\"cred-number\" /> */}\n            <input\n              id=\"ccn\"\n              type=\"tel\"\n              inputmode=\"numeric\"\n              pattern=\"[0-9\\s]{13,19}\"\n              autocomplete=\"cc-number\"\n              maxlength=\"19\"\n              placeholder=\"xxxx xxxx xxxx xxxx\"\n              onChange={(e) => {\n                let num = e.target.value;\n                const last = num[num.length - 1];\n                if (isNaN(Number(last) || last !== ' ')) {\n                  e.target.value = num.slice(0, num.length - 1);\n                }\n              }}\n              required\n            />\n\n            <label htmlFor=\"cvc\">CVC: </label>\n            <input\n              type=\"text\"\n              name=\"cvc\"\n              maxLength=\"3\"\n              minLength=\"3\"\n              onChange={(e) => {\n                let num = e.target.value;\n                const last = num[num.length - 1];\n                if (isNaN(Number(last))) {\n                  e.target.value = num.slice(0, num.length - 1);\n                }\n              }}\n              required\n            />\n          </div>\n\n          <label htmlFor=\"cred-date\">Data de validade: </label>\n          <input\n            type=\"text\"\n            onChange={(e) => {\n              let num = e.target.value;\n              const last = num[num.length - 1];\n              if (isNaN(Number(last))) {\n                e.target.value = num.slice(0, num.length - 1);\n              }\n            }}\n            placeholder=\"Ex: MM/YYYY\"\n          />\n        </div>\n      );\n    case 'credit':\n      return;\n    case 'boleto':\n      return;\n    case 'transfer':\n      return;\n    case 'pix':\n      return;\n\n    default:\n      return <div>test</div>;\n  }\n}\n\nexport default FormaPagamento;\n",{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"rule